{
  "manifest_version": 3,
  "name": "VIT WiFi Auto Login",
  "version": "1.1",
  "permissions": [
    "storage",
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "https://phc.prontonetworks.com/*",
    "http://phc.prontonetworks.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
